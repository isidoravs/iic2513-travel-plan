<h2>Destinations</h2>
  <table id="one-destination">
    <thead>
        <tr>
          <th>Name</th>
          <th>Picture</th>
          <th colspan="2">Actions</th>
        </tr>
    </thead>
    <tbody>
        <tr>
          <td><%- destination.destinationName %></td>
          <td><%- destination.destinationPicture %></td>
          <td><a href="<%- editDestinationPath(destination) %>">Edit</a></td>
          <td>
            <form action="<%- deleteDestinationPath(destination) %>" method="POST">
              <input type="hidden" name="_method" value="delete" />
              <input type="submit" value="Delete" />
            </form>
          </td>
        </tr>
    </tbody>
  </table>
