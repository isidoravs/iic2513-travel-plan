<h2><%= userName %>'s itineraries</h2>
<table id="generic-list">
    <thead>
        <tr>
          <th>N</th>
          <th>Name</th>
        </tr>
    </thead>
    <tbody>
      <% itinerariesList.forEach((itinerary, index) => { %>
        <tr>
          <td><%- (index + 1) %></td>
          <td><%- itinerary.itineraryName %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  
  <ul>
    <li><a href="<%= usersListPath %>">Back to Users List</a></li>
  </ul>
  
